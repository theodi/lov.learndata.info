{% macro Link(term) -%}
    {% if term.pending %}{% set CLASS = "ext ext-pending" %}
    {% elif term.retired %}{% set CLASS = "ext ext-attic" %}
    {% else %}{% set CLASS = "core" %}
    {% endif %}
    {% if term.id %}{% set name = term.id %}{% else %}{% set name = term %}{% endif %}
    <a href="#term_{{name}}" class="{{CLASS}}" title="{{name}}">{{name}}</a>
{%- endmacro %}



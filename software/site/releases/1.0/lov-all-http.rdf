<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:schema="http://schema.org/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
>
  <rdf:Property rdf:about="http://schema.org/alternateName">
    <rdfs:label>alternateName</rdfs:label>
    <rdfs:comment>An alias for the item.</rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/subjectOf">
    <schema:rangeIncludes rdf:resource="http://schema.org/Event"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/CreativeWork"/>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
    <rdfs:comment>A CreativeWork or Event about this Thing.</rdfs:comment>
    <schema:inverseOf rdf:resource="http://schema.org/about"/>
    <rdfs:label>subjectOf</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/potentialAction">
    <rdfs:comment>Indicates a potential Action, which describes an idealized action in which this thing would play an 'object' role.</rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/Action"/>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
    <rdfs:label>potentialAction</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/name">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#label"/>
    <rdfs:comment>The name of the item.</rdfs:comment>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
    <owl:equivalentProperty rdf:resource="http://purl.org/dc/terms/title"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <rdfs:label>name</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/url">
    <rdfs:label>url</rdfs:label>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/URL"/>
    <rdfs:comment>URL of the item.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:about="https://lov.learndata.info/payee">
    <rdfs:label>payee</rdfs:label>
    <rdfs:comment>The entity to which the payment was made.</rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Organization"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Person"/>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Payment"/>
  </rdf:Property>
  <rdf:Property rdf:about="https://lov.learndata.info/referencesInvoice">
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Payment"/>
    <rdfs:label>related invoice</rdfs:label>
    <rdfs:comment>The invoice related to the payment</rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/Invoice"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/identifier">
    <rdfs:label>identifier</rdfs:label>
    <rdfs:comment>The identifier property represents any kind of identifier for any kind of [[Thing]], such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See [background notes](/docs/datamodel.html#identifierBg) for more details.
        </rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/PropertyValue"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/URL"/>
    <owl:equivalentProperty rdf:resource="http://purl.org/dc/terms/identifier"/>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/additionalType">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#type"/>
    <rdfs:label>additionalType</rdfs:label>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
    <rdfs:comment><![CDATA[An additional type for the item, typically used for adding more specific types from external vocabularies in microdata syntax. This is a relationship between something and a class that the thing is in. Typically the value is a URI-identified RDF class, and in this case corresponds to the
    use of rdf:type in RDF. Text values can be used sparingly, for cases where useful information can be added without their being an appropriate schema to reference. In the case of text values, the class label should follow the schema.org <a href="http://schema.org/docs/styleguide.html">style guide</a>.]]></rdfs:comment>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/URL"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/description">
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/TextObject"/>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
    <rdfs:label>description</rdfs:label>
    <owl:equivalentProperty rdf:resource="http://purl.org/dc/terms/description"/>
    <rdfs:comment>A description of the item.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/disambiguatingDescription">
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <rdfs:comment>A sub property of description. A short description of the item used to disambiguate from other, similar items. Information from other properties (in particular, name) may be necessary for the description to be useful for disambiguation.</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="https://lov.learndata.info/description"/>
    <rdfs:label>disambiguatingDescription</rdfs:label>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/sameAs">
    <schema:rangeIncludes rdf:resource="http://schema.org/URL"/>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
    <rdfs:label>sameAs</rdfs:label>
    <rdfs:comment>URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:about="https://lov.learndata.info/paymentDate">
    <schema:rangeIncludes rdf:resource="http://schema.org/Date"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/DateTime"/>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Payment"/>
    <rdfs:label>payment date</rdfs:label>
    <rdfs:comment>The date the payment is deemed to have occurred.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/image">
    <rdfs:label>image</rdfs:label>
    <rdfs:comment>An image of the item. This can be a [[URL]] or a fully described [[ImageObject]].</rdfs:comment>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/ImageObject"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/URL"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://schema.org/mainEntityOfPage">
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Thing"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/CreativeWork"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/URL"/>
    <rdfs:label>mainEntityOfPage</rdfs:label>
    <schema:inverseOf rdf:resource="http://schema.org/mainEntity"/>
    <rdfs:comment>Indicates a page (or other CreativeWork) for which this thing is the main entity being described. See [background notes](/docs/datamodel.html#mainEntityBackground) for details.</rdfs:comment>
  </rdf:Property>
  <rdfs:Class rdf:about="https://lov.learndata.info/Property">
    <rdfs:comment>A property, used to indicate attributes and relationships of some Thing; equivalent to rdf:Property.</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <schema:isPartOf rdf:resource="http://meta.schema.org"/>
    <rdfs:subClassOf rdf:resource="https://lov.learndata.info/Intangible"/>
    <rdfs:label>Property</rdfs:label>
  </rdfs:Class>
  <rdf:Property rdf:about="https://lov.learndata.info/payer">
    <rdfs:label>payee</rdfs:label>
    <schema:rangeIncludes rdf:resource="http://schema.org/Text"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Organization"/>
    <schema:rangeIncludes rdf:resource="http://schema.org/Person"/>
    <rdfs:comment>The entity to which the payment was made.</rdfs:comment>
    <schema:domainIncludes rdf:resource="https://lov.learndata.info/Payment"/>
  </rdf:Property>
  <rdfs:Class rdf:about="https://lov.learndata.info/Payment">
    <rdfs:label>Payment</rdfs:label>
    <rdfs:comment>A payment between payee and payer for an amount. Otherwise known as a transaction.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://lov.learndata.info/Intangible"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://lov.learndata.info/Thing">
    <rdfs:label>Thing</rdfs:label>
    <owl:sameAs rdf:resource="http://schema.org/Thing"/>
    <rdfs:comment>The most generic type of item.</rdfs:comment>
    <schema:sameAs rdf:resource="http://schema.org/Thing"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://lov.learndata.info/Intangible">
    <schema:sameAs rdf:resource="http://schema.org/Intangible"/>
    <rdfs:comment>A utility class that serves as the umbrella for a number of 'intangible' things such as quantities, structured values, etc.</rdfs:comment>
    <owl:sameAs rdf:resource="http://schema.org/Intangible"/>
    <rdfs:subClassOf rdf:resource="https://lov.learndata.info/Thing"/>
    <rdfs:label>Intangible</rdfs:label>
  </rdfs:Class>
</rdf:RDF>
